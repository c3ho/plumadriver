{
  "name": "plumadriver",
  "version": "0.2.0",
  "description": "",
  "main": "./build/index.js",
  "dependencies": {
    "async-mutex": "^0.1.3",
    "body-parser": "^1.18.3",
    "encodeurl": "^1.0.2",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "express_logger": "0.0.4",
    "jsdom": "^15.1.1",
    "lzutf8": "^0.5.5",
    "mime-types": "^2.1.24",
    "minimist": "^1.2.0",
    "os": "^0.1.1",
    "request": "^2.88.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "uglify-js": "^3.7.2",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "validator-js": "^0.2.1",
    "vm2": "^3.8.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "commitlint": "^8.1.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fs": "0.0.1-security",
    "http-post": "^0.1.1",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.2",
    "lint-staged": "^9.2.5",
    "mocha": "^6.1.4",
    "nock": "^11.7.0",
    "nodemon": "^1.18.9",
    "pkg": "^4.4.0",
    "prettier": "^1.18.2",
    "should": "^13.2.3",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/",
      "/node_modules/"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:compile": "tsc && jest",
    "test:mocha": "mocha --exit",
    "lint": "eslint",
    "start": "node ./build/index.js",
    "compile": "tsc",
    "build:linux": "tsc && pkg . --target latest-linux-x64",
    "build:win": "tsc && pkg . --target latest-win-x64",
    "build:macos": "tsc && pkg . --target latest-mac-x64",
    "build:all": "tsc && pkg . --target latest-linux-x64,latest-win-x64,latest-mac-x64",
    "generate-docs": "./node_modules/.bin/typedoc --out docs ./src/ --options ./typedoc.json &>/dev/null",
    "watch": "nodemon --watch src/ --exec 'npm run compile-ts && npm start' -e ts"
  },
  "bin": "bin.js",
  "author": "OSTEP",
  "contributors": [
    "Miguel Roncancio (https://github.com/miggs125)",
    "Pouya Oftadeh (https://github.com/poftadeh)"
  ],
  "license": "MIT",
  "pkg": {
    "assets": [
      "./node_modules/vm2",
      "./node_modules/@types",
      "./node_modules/typescript/lib/*.d.ts",
      "./src/**/*.ts",
      "./tsconfig.json"
    ]
  }
}
